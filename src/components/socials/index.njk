<ul class="socials {{ mix }}">
  {% for item in socials %}
    <li class="socials__item">
      <a class="socials__link" href="{{ item.href }}" style="background-image: url(img/sprite.min.svg#{{ item.icon }})">
        <span class="visually-hidden">{{ item.title }}</span>
      </a>
    </li>
  {% endfor %}
</ul>
